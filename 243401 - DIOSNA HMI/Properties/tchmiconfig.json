    {
      "event": "onInitialized",
      "actions": [
        {
          "objectType": "Comment",
          "comment": "Get current user and group in PLC"
        },
        {
          "objectType": "JavaScript",
          "active": true,
          "sourceLines": [
            "var clientIp = document.location.hostname;",
            "console.log(\"Client IP:\", clientIp);",
            "",
            "var user = TcHmi.Server.getCurrentUser();",
            "var group = TcHmi.Server.getGroupsOfCurrentUser();",
            "var symbol_user = new TcHmi.Symbol('%s%PLC1.GV.sUserLogged%/s%');",
            "",
            "if (user) {",
            "    symbol_user.write(user, function (data) {    ",
            "        console.log(\"User written:\", data);",
            "    });",
            "} else {",
            "    console.log(\"Not a local IP\");",
            "}"
          ],
          "asyncWait": true,
          "waitMode": "Synchronous"
        },
        {
          "objectType": "WriteToSymbol",
          "active": true,
          "symbolExpression": "%s%PLC1.GV.stHMI.stMachine_Ctrl.bLogIn%/s%",
          "value": {
            "objectType": "StaticValue",
            "valueType": "tchmi:general#/definitions/BOOL",
            "value": true
          },
          "asyncWait": true
        },
        {
          "objectType": "Comment",
          "comment": "Read all recipes"
        },
        {
          "objectType": "JavaScript",
          "active": true,
          "sourceLines": [
            "TcHmi.Server.RecipeManagement.listRecipes(function (data) {",
            "    if (data.error === 0) {",
            "        const recipes = data.value.Production;",
            "        console.log(recipes);",
            "        const productionRecipes = [];",
            "",
            "        // Loop through all recipes",
            "        for (const recipeName in recipes) {",
            "            if (recipes.hasOwnProperty(recipeName)) {",
            "                const recipe = recipes[recipeName];",
            "",
            "                // Check if recipeTypeName is 'Production'",
            "                if (recipe.recipeTypeName === 'Production') {",
            "                    productionRecipes.push(recipeName);",
            "                }",
            "            }",
            "        }",
            "",
            "        // Write only Production recipe names to the symbol",
            "        TcHmi.Symbol.writeEx('%i%RecipeList_Prod%/i%', productionRecipes, function (result) {",
            "            if (result.error === TcHmi.Errors.NONE) {",
            "                //console.log('Production recipe names updated successfully:', productionRecipes);",
            "            } else {",
            "                console.error('Failed to write production recipe names to symbol:', result.errorMessage);",
            "            }",
            "        });",
            "    } else {",
            "        console.error('Error fetching recipes:', data.errorMessage);",
            "    }",
            "});"
          ],
          "asyncWait": true,
          "waitMode": "Synchronous"
        },
        {
          "objectType": "JavaScript",
          "active": true,
          "sourceLines": [
            "TcHmi.Server.RecipeManagement.listRecipes(function (data) {",
