        "symbolExpression": "%ctrl%PopUp_DevControl::TargetFile%/ctrl%",
        "value": {
          "objectType": "StaticValue",
          "valueType": "tchmi:framework#/definitions/TcHmi.Controls.System.TcHmiPopup.TargetFile",
          "value": {
            "path": "UserControls/Faceplate_GeneralCoil.usercontrol",
            "attributes": {
              "data-tchmi-devobj": "%pp%Device_Object|BindingMode=TwoWay%/pp%",
              "data-tchmi-interlockstext": "%pp%Interlocks_Text%/pp%",
              "data-tchmi-alarmstext": "%pp%Alarms_Text%/pp%",
              "data-tchmi-alarmsrecovertext": "%pp%Alarms_Recover_Text%/pp%",
              "data-tchmi-hp_text": "%pp%HP_Text%/pp%",
              "data-tchmi-wp_text": "%pp%WP_Text%/pp%"
            }
          }
        },
        "asyncWait": true
      },
      {
        "objectType": "WriteToSymbol",
        "active": true,
        "symbolExpression": "%ctrl%PopUp_DevControl::HeaderText%/ctrl%",
        "value": {
          "objectType": "Symbol",
          "valueType": "tchmi:general#/definitions/String",
          "symbolExpression": "%pp%Device_Name%/pp%"
        },
        "asyncWait": true
      },
      {
        "objectType": "ControlApiFunction",
        "active": true,
        "control": "PopUp_DevControl",
        "fn": "open",
        "asyncWait": true
      },
      {
        "objectType": "WriteToSymbol",
        "active": true,
        "symbolExpression": "%ctrl%PopUp_DevControl::Height%/ctrl%",
        "value": {
          "objectType": "StaticValue",
          "valueType": "tchmi:framework#/definitions/MeasurementValue",
          "value": 500.0
        },
        "asyncWait": true
      },
      {
        "objectType": "WriteToSymbol",
        "active": true,
        "symbolExpression": "%ctrl%PopUp_DevControl::Width%/ctrl%",
        "value": {
          "objectType": "StaticValue",
          "valueType": "tchmi:framework#/definitions/MeasurementValue",
          "value": 290.0
        },
        "asyncWait": true
      }
    ]
  }
]</script>
