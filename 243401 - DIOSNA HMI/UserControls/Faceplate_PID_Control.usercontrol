<script data-tchmi-target-attribute="data-tchmi-trigger" type="application/json">[
  {
    "event": "%pp%Obj::i_rSetpoint%/pp%",
    "actions": [
      {
        "objectType": "JavaScript",
        "active": true,
        "sourceLines": [
          "//PID object parameters",
          "var sym_HA = new TcHmi.Symbol('%pp%Obj::i_iHighAlarm%/pp%');",
          "var sym_HW = new TcHmi.Symbol('%pp%Obj::i_iHighWarning%/pp%');",
          "var sym_LW = new TcHmi.Symbol('%pp%Obj::i_iLowWarning%/pp%');",
          "var sym_LA = new TcHmi.Symbol('%pp%Obj::i_iLowAlarm%/pp%');",
          "var sym_SP = new TcHmi.Symbol('%pp%Obj::i_rSetpoint%/pp%');",
          "//local parameters used only for gauge",
          "var sym_GaugeMin = new TcHmi.Symbol('%pp%Gauge_Min%/pp%');",
          "var sym_GaugeMax = new TcHmi.Symbol('%pp%Gauge_Max%/pp%');",
          "var sym_Gauge_HA = new TcHmi.Symbol('%pp%Gauge_HighAlarm%/pp%');",
          "var sym_Gauge_HW = new TcHmi.Symbol('%pp%Gauge_HighWarning%/pp%');",
          "var sym_Gauge_LW = new TcHmi.Symbol('%pp%Gauge_LowWarning%/pp%');",
          "var sym_Gauge_LA = new TcHmi.Symbol('%pp%Gauge_LowAlarm%/pp%');",
          "var sym_MainTicks = new TcHmi.Symbol('%pp%GaugeTicksRange::mainTickRange%/pp%');",
          "var sym_SubTicks = new TcHmi.Symbol('%pp%GaugeTicksRange::subTickRange%/pp%');",
          "var sym_StepRange = new TcHmi.Symbol('%pp%GaugeStepRange%/pp%');",
          "",
          "var HA = sym_HA.read();",
          "var HW = sym_HW.read();",
          "var LW = sym_LW.read();",
          "var LA = sym_LA.read();",
          "var SP = sym_SP.read();",
          "",
          "//calculation",
          "sym_GaugeMin.write(SP-LA-LA);",
          "sym_GaugeMax.write(SP+HA+HA);",
          "sym_Gauge_HA.write(SP+HA);",
          "sym_Gauge_HW.write(SP+HW);",
          "sym_Gauge_LW.write(SP-LW);",
          "sym_Gauge_LA.write(SP-LA);",
          "",
          "var minRange = sym_GaugeMin.read();",
          "var maxRange = sym_GaugeMax.read();",
          "",
          "sym_MainTicks.write((maxRange-minRange)/5);",
          "sym_SubTicks.write((maxRange-minRange)/10);",
          "sym_StepRange.write((maxRange-minRange)/5);",
          "",
          "// TROUBLESHOOTING LINES",
          "//console.log('MIN',sym_GaugeMin.read());",
          "//console.log('MAX',sym_GaugeMax.read());",
          "//console.log('HA',sym_Gauge_HA.read());",
          "//console.log('HW',sym_Gauge_HW.read());",
          "//console.log('LW',sym_Gauge_LW.read());",
          "//console.log('LA',sym_Gauge_LA.read());"
        ],
        "injectContextObject": false,
        "asyncWait": true,
        "waitMode": "Synchronous"
      }
    ]
  }
]</script>
